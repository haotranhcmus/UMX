{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@/components/*": ["src/components/*"],
      "@assets/*": ["assets/*"],
      "@/constants/*": ["src/constants/*"],
      "@/navigation/*": ["src/navigation/*"],
      "@/utils/*": ["src/utils/*"],
      "@/theme/*": ["src/theme/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "nativewind-env.d.ts"]
}
